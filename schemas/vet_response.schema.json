{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "WildGuard Vet Response",
  "type": "object",
  "required": ["decision", "reason", "expected_treatment_time"],
  "properties": {
    "decision": { "type": "string", "enum": ["accept", "reject"] },
    "reason": { "type": "string" },
    "expected_treatment_time": { "type": "integer", "minimum": 0 }
  },
  "additionalProperties": true
}
