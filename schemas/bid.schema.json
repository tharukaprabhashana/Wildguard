{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "WildGuard Ranger Unit Bid",
  "type": "object",
  "required": ["unit_id", "eta_minutes", "equipment_ready", "estimated_success", "cost"],
  "properties": {
    "unit_id": { "type": "string" },
    "eta_minutes": { "type": "integer", "minimum": 0 },
    "equipment_ready": { "type": "boolean" },
    "estimated_success": { "type": "number", "minimum": 0, "maximum": 1 },
    "cost": { "type": "number", "minimum": 0 }
  },
  "additionalProperties": true
}
